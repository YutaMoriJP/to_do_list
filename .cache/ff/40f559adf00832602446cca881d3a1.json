\n    setText(prevNote => ({ ...prevNote, [name]: value, id: Date.now() }));\n  };\n  const handleSubmit = e => {\n    e.preventDefault();\n    setNote(prevNote => [...prevNote, text]);\n  };\n  const handleClick = uniqId => {\n    setNote(prevNote => prevNote.filter(({ id }) => id !== uniqId));\n  };\n  return (\n    <div>\n      <h1 style={{ textAlign: 'center' }}>To Do List:</h1>\n      <p>\n        {!note.length\n          ? 'Currently Empty'\n          : note.map(({ note, id }) => (\n              <div>\n                <button onClick={() => handleClick(id)}>Remove</button>\n                <p>{note}</p>\n              </div>\n            ))}\n      </p>\n      <input\n        type=\"text\"\n        value={text.note}\n        onChange={handleChange}\n        name=\"note\"\n      />\n      <button onClick={handleSubmit}>Submit</button>\n    </div>\n  );\n};\n\nexport { DisplayNotes };\n"},"lineCount":null}},"error":null,"hash":"ea8e610a705e8b687c9f4a829652fe98","cacheData":{"env":{}}}